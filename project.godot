config_version=5

[application]

config/name="PokemonClone"
run/main_scene="res://bootup.tscn"
config/features=PackedStringArray("4.2", "C#", "Mobile")
config/icon="res://icon.svg"

[autoload]

AutoloadStartup="*res://Autoloads/AutoloadStartup.cs"
ResourceDatabase="*res://Autoloads/ResourceDatabase.tscn"
TileMapController="*res://Autoloads/tile_map_controller.tscn"
PokemonController="*res://Autoloads/PokemonController.cs"
SaveDataController="*res://Autoloads/SaveDataController.cs"
SceneChanger="*res://Autoloads/scene_changer.tscn"
AnimationController="*res://ui/Animations/Animation.tscn"
MusicController="*res://Autoloads/music_controller.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/per_pixel_transparency/allowed=true

[dotnet]

project/assembly_name="PokemonClone"

[filesystem]

import/blender/enabled=false

[gui]

theme/custom_font="res://Fonts/days/Days.otf"
theme/default_font_hinting=2

